SQL> CREATE OR REPLACE FUNCTION ppb_karyawan (p_id employees.employee_id%TYPE)
  2  RETURN NUMBER IS
  3      v_gaji employees.salary%TYPE;
  4  BEGIN
  5      SELECT salary INTO v_gaji FROM employees WHERE employee_id = p_id;
  6      v_pajak := v_gaji * 0.1;
  7      RETURN v_gaji;
  8  END;
  9  /

