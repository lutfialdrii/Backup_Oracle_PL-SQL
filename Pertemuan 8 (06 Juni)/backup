SQL> CREATE OR REPLACE FUNCTION luas_lingkaran(p_jari_jari NUMBER)
  2  RETURN FLOAT IS
  3  v_phi NUMBER := 3.14;
  4  v_luas NUMBER := v_phi * p_jari_jari * p_jari_jari;
  5  BEGIN
  6  RETURN v_luas;
  7  END;
  8  /

