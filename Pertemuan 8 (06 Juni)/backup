SQL> CREATE OR REPLACE FUNCTION pph_karyawan (p_id employees.employee_id%TYPE)
  2  RETURN NUMBER IS
  3      v_gaji employees.salary%TYPE;
  4      v_pajak NUMBER;
  5  BEGIN
  6      SELECT salary INTO v_gaji FROM employees WHERE employee_id = p_id;
  7      v_pajak := v_gaji * 0.1;
  8      RETURN v_pajak;
  9  END;
 10  /

Function created.

